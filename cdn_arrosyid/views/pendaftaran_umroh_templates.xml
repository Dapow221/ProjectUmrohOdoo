                                                    <div class="form-group">
                                                        <label>vaksin mengitis</label>
                                                        <input type="text" class="form-control" id="meningitis"/>
                                                    </div>
                                                </div>
                                                <div class="col-6">
                                                    <div class="form-group">
                                                        <label>vaksin covid</label>
                                                        <input type="text" class="form-control" id="covid"/>
                                                    </div>
                                                </div>
                                                <div class="col-6">
                                                    <div class="form-group">
                                                        <label>Sudah Umroh</label>
                                                        <input type="text" class="form-control" id="sudah_umroh"/>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                <!-- </div> -->
                                <br/><br/>
                                <h4 class="p-3"><b><u>Paket Umroh</u></b></h4>
                                <div class="p-3 row">
                                    <div class="col-6">
                                        <div class="form-group">
                                            <label for="paket_umroh">Pilih Paket Umroh</label>
                                            <select class="form-control" id="paket_umroh">
                                                <option value="" selected="1" disabled="1">Pilih Paket Umroh</option>
                                                <t t-foreach="data_paket_umroh" t-as="paket">
                                                    <option t-att-value="paket.id"><t t-esc="paket.name"/></option>
                                                </t>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <div class="form-group">
                                            <label for="paket_umroh">Pilih Sesi</label>
                                            <select class="form-control" id="sesi_umroh">
                                                <option value="" selected="1" disabled="1">Pilih Sesi Umroh</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <div class="form-group">
                                            <label for="harga">Harga</label>
                                            <input type="text" class="form-control" id="harga" readonly="1"/>
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <div class="d-grid gap-2">
                                            <button class="btn btn-outline-primary m-3" type="button" id="i" data-bs-toggle="modal" data-bs-target="#detail_sesi">Detail</button>
                                            <div class="modal fade" id="detail_sesi" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                                <div class="modal-dialog modal-lg">
                                                  <div class="modal-content">
                                                    <div class="modal-header">
                                                      <h5 class="modal-title" id="exampleModalLabel">Detail Paket Umroh</h5>
                                                      <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                                    </div>
                                                    <div class="modal-body">
                                                        <div class="card">
                                                            <img src="https://ibenimages.com/wp-content/uploads/2012/06/20120628-082434.jpg" class="card-img-top" alt=""/>
                                                            <div class="card-body">
                                                              <h5 class="card-title" id="nama_sesi_modal">Judul</h5>
                                                              <p class="card-text" id="keterangan_sesi">Keterangan.</p>
                                                            </div>
                                                            <ul class="list-group list-group-flush row">
                                                                <li class="list-group-item col-12">Tanggal dimuai dan berakhir : <span id="tgl_brk"></span> s/d <span id="tgl_plg"></span></li>
                                                                <li class="list-group-item col-6">Pembimbing : <span id="pembimbing"></span></li>
                                                              <li class="list-group-item col-6">Maskapai : <span id="maskapai"></span></li>
                                                              <li class="list-group-item col-6">Petugas :<span id="petugas"></span> </li>
                                                              <li class="list-group-item col-6">Hotel : <span id="hotel"></span></li>
                                                            </ul>
                                                            <div class="card-body">
                                                              <a href="#" class="card-link">Rencana perjalanan</a>
                                                              <a href="#" class="card-link">Jamaah terdaftar</a>
                                                            </div>
                                                          </div>
                                                    </div>
                                                    <div class="modal-footer">
                                                      <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                                                      <button type="button" class="btn btn-primary">Send message</button>
                                                    </div>
                                                  </div>
                                                </div>
                                              </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 mt-2">
                                    <div class="d-grid gap-2">
                                        <button class="btn btn-primary" type="submit" id="submit_pendaftaran">Daftar</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </t>
        </template>

