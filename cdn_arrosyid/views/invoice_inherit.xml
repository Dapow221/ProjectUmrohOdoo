<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <!-- purchase.order.training inherit form view -->
        <record id="account_move_travel_inherit" model="ir.ui.view">
            <field name="name">account.move.travel.inherit.view.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <!-- Add your xpath here -->
                <field name="ref" position="after">
                    <field name="product_travel"/>
                </field>
                <xpath expr="//field[@name='product_id']" position="attributes">
                    <attribute name="domain">
                        [('product_travel', '=', parent.product_travel)]
                    </attribute>
                </xpath>
            </field>
        </record>
    
    </data>
    

</odoo>
